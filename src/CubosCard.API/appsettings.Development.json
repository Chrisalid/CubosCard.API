{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DetailedErrors": true,
  "ConnectionStrings": {
    "ApplicationDbContext": ""
  },
  "DbContextOptions": {
		"SensitiveDataLoggingEnabled": true,
		"DetailedErrorsEnabled": true
	},
  "Resilience": {
		"Enabled": true,
		"MaxRetryAttempts": 5,
		"Timeout": 30
	}
}
